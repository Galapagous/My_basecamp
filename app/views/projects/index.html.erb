<html>
<head>
<%=stylesheet_link_tag "projectMain"%>
</head>
<body>
<p style="color: green"><%= notice %></p>

<h1>Projects</h1>
<div class="top">
<%= link_to new_project_path do%>
  <%= image_tag("add.png", class: "img-fluid", style: "width: 30px; height: 30px; border-radius: 1rem; object-fit: cover; margin-right: 10px;") %>
  <span>Add New</span>
<%end%>
</div>
<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
  <% end %>
</div>
</body>
</html>